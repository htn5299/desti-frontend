{
    "extends": ["react-app", "prettier"],
    "plugins": ["react", "prettier"],
    "rules": {
      "prettier/prettier": [
        "warn",
        {
          "arrowParens": "always",
          "semi": false,
          "trailingComma": "none",
          "tabWidth": 2,
          "endOfLine": "auto",
          "useTabs": false,
          "jsxSingleQuote": true,
          "singleQuote": true,
          "printWidth": 120
        }
      ]
    }
  }